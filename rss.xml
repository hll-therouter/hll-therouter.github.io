---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title><![CDATA[ {{ site.title | xml_escape }}]]></title>
    <description><![CDATA[{{ site.description | xml_escape }}]]></description>
    <link><![CDATA[ https://therouter.cn/]]></link>
    <pubDate><![CDATA[{{ site.time | date: "%Y-%m-%d" }}]]></pubDate>
    <lastBuildDate><![CDATA[{{ site.time | date: "%Y-%m-%d" }}]]></lastBuildDate>
    {% for post in site.posts limit:50 %}
    <item>
        <title><![CDATA[{{ post.title | xml_escape }}]]></title>
        <description><![CDATA[https://therouter.cn{{ post.url }}</br>{{ post.description | xml_escape }}]]></description>
        <recommend>{{ post.recommend }}</recommend>
        <image><![CDATA[{{ post.image }}]]></image>
        <pubDate><![CDATA[{{ post.date | date: "%Y-%m-%d" }}]]></pubDate>
        <link><![CDATA[https://therouter.cn{{ post.url }}]]></link>
        <tags>
          {% for tag in post.tags %}
          <tag>{{ tag | xml_escape }}</tag>
          {% endfor %}
        </tags>
        <columnNum><![CDATA[{{ post.column[0] | xml_escape }}]]></columnNum>
        <column><![CDATA[{{ post.column[1] | xml_escape }}]]></column>
        <categories>
          {% for cat in post.categories %}
          <category>{{ cat | xml_escape }}</category>
          {% endfor %}
        </categories>
        <content><![CDATA[https://therouter.cn{{ post.url }}</br>{{ post.content | xml_escape }}]]></content>
    </item>
    {% endfor %}
  </channel>
</rss>
